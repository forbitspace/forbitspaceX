{
  "name": "forbitspacex",
  "version": "2.0.0",
  "main": "hardhat.config.ts",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --write .",
    "clean": "hardhat clean && rm -rf contracts",
    "compile": "hardhat compile",
    "build": "sol-merger --export-plugin SPDXLicenseRemovePlugin ./contracts-upgradeable/forbitspaceX.sol ./contracts",
    "deploy": "hardhat run scripts/deploy_verify.ts --network rinkeby",
    "update": "hardhat run scripts/upgrade.ts --network rinkeby",
    "swap": "hardhat run scripts/swap.ts --network rinkeby"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "hardhat": "^2.6.8",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "sol-merger": "^3.1.0",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.2"
  }
}
