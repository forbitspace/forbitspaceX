{
  "name": "forbitspacex",
  "version": "2.0.2",
  "main": "hardhat.config.ts",
  "license": "MIT",
  "scripts": {
    "clean": "hardhat clean",
    "prettier": "prettier --write .",
    "build-contracts": "sol-merger --export-plugin SPDXLicenseRemovePlugin ./contracts-uups/*.sol ./contracts-merged",
    "precompile:off": "npm run build-contracts",
    "compile": "hardhat compile",
    "update": "hardhat run scripts/upgrade.ts --network rinkeby",
    "deploy:t": "hardhat run scripts/deploy-transparent.ts --network rinkeby",
    "deploy:u": "hardhat run scripts/deploy-uups.ts --network rinkeby",
    "verify": "hardhat run scripts/verify.ts --network rinkeby",
    "swap": "hardhat run scripts/swap_ethers.ts --network rinkeby"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.11.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.4",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "sol-merger": "^3.1.0",
    "ts-node": "^10.2.1",
    "tsc": "^2.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.2"
  }
}
